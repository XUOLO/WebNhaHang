@model IEnumerable<WebNhaHang.Models.EF.ProductCategory>

@if (Model != null && Model.Any())
{

<div class="menu-btns">

    @foreach (var item in Model)
    {
        if (ViewBag.CateId == item.id)
        {

            <p type="button" class="menu-btn active-btn" id="@item.Alias">@item.Title</p>

        }
        else
        {
            <p type="button" class="menu-btn" id="@item.Alias">@item.Title</p>
            @*<li><a href="/danh-muc-san-pham/@item.Alias-@(item.id)">@item.Title</a></li>*@

        }
    }



</div>
}

 